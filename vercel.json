{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      }
    ],
    "functions": {
      "api/index.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 60
      }
    }
  }
  